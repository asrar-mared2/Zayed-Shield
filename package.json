  "scripts": {
    "start": "node src/index.js",
    "dev": "nodemon src/index.js",
    "build": "npm run clean && npm run compile",
    "compile": "babel src -d dist",
    "clean": "rimraf dist coverage .nyc_output",
    "test": "npm run test:unit && npm run test:integration && npm run test:security",
    "test:unit": "jest --coverage --config jest.config.js",
    "test:integration": "jest --config jest.integration.config.js",
    "test:security": "npm run test:vulnerability && npm run test:sast",
    "test:vulnerability": "npm audit --audit-level=moderate",
    "test:sast": "eslint . --ext .js,.jsx,.ts,.tsx",
    "test:watch": "jest --watch",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "lint:check": "eslint . --ext .js,.jsx,.ts,.tsx",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "format:check": "prettier --check \"src/**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "security:audit": "npm audit fix",
    "security:check": "snyk test",
    "security:monitor": "snyk monitor",
    "security:scan": "trivy fs . --severity HIGH,CRITICAL",
    "dependency:check": "npm outdated",
    "dependency:update": "npm-check-updates -u",
    "docker:build": "docker build -t dere3-zayed:latest .",
    "docker:run": "docker run -p 3000:3000 dere3-zayed:latest",
    "docker:compose": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "db:migrate": "sequelize-cli db:migrate",
    "db:seed": "sequelize-cli db:seed:all",
    "db:reset": "sequelize-cli db:migrate:undo:all && npm run db:migrate && npm run db:seed",
    "logs": "pm2 logs dere3-zayed",
    "monitor": "pm2 monit",
    "deploy": "npm run build && npm run deploy:production",
    "deploy:production": "pm2 start ecosystem.config.js --env production",
    "deploy:staging": "pm2 start ecosystem.config.js --env staging",
    "backup": "node scripts/backup.js",
    "restore": "node scripts/restore.js",
    "docs": "jsdoc -c jsdoc.json",
    "docs:serve": "docsify serve docs",
    "prepare": "husky install",
    "precommit": "lint-staged",
    "prepush": "npm run test && npm run lint:check",
    "release": "standard-version",
    "release:minor": "standard-version --release-as minor",
    "release:major": "standard-version --release-as major"
  },
